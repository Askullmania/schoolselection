- admission = school.grade(session[:grade_level])
%td.tabbable.admissions.hidden.larger
  =raw admissions.try(:open_seats) ? admissions.open_seats : '&nbsp;'
%td.tabbable.admissions.hidden.larger
  =raw admissions.try(:first_choice) ? admissions.first_choice : '&nbsp;'
%td.tabbable.admissions.hidden.larger
  =raw admissions.try(:second_choice) ? admissions.second_choice : '&nbsp;'
%td.tabbable.admissions.hidden.larger
  =raw admissions.try(:third_choice) ? admissions.third_choice : '&nbsp;'