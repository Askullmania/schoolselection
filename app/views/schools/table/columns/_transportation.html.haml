%td.tabbable.transportation.hidden.large
  =raw school.distance.present? ? "#{distance_in_miles_from_meters(school.distance)} mi." : '&nbsp;'
%td.tabbable.transportation.hidden.large.center
  - if school.eligibility =~ /Walk Zone/
    =raw school.distance.present? ? "#{walk_time(school.distance)} min." : '&nbsp;'
  - else
    &nbsp;
%td.tabbable.transportation.hidden.large
  =raw school.distance.present? ? "#{drive_time(school.distance)} min." : '&nbsp;'
%td.tabbable.transportation.hidden.large
  - if @grade_level.name == "High"
    MBTA time
  - else
    =raw school.eligibility =~ /Walk Zone/ ? '&nbsp;' : "Elgible"
  